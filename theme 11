<!-- navigation -->

<script>
$(document).ready(function() {
    $('.nav #trigger').click(function() {
        $('.entries').fadeOut();
        $('.popup').fadeIn();
    });
    
    $('.popup #close').click(function() {
        $('.popup').fadeOut();
        $('.entries').fadeIn();
    });
});
</script>

